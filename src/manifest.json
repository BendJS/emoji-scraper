{
    "name": "Bend JS - Slack Emoji Scraper",
    "short_name": "Emoji Scraper",
    "permissions": ["activeTab"],
    "content_scripts": [{
        "js": ["main.js"],
        "matches": ["*://*.slack.com/*/emoji*"],
        "run_at": "document_end"
    }],
    "icons": {
      "128": "images/icon_128.png"
    },
    "manifest_version": 2,
    "web_accessible_resources": [
      "main.js",
      "main.js.map",
      "images/icon_128.png",
      "main.css",
      "main.css.map"
    ]
}